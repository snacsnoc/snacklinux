 #!/bin/bash
  # put this in a file called createdev
  #
  cp -avp /dev/console   dev
  cp -avp /dev/core   dev
  cp -avp /dev/fd0   dev
  cp -avp /dev/null   dev
  cp -avp /dev/ptmx   dev
  cp -avp /dev/pts   dev
  cp -avp /dev/ram0   dev
  cp -avp /dev/ram1   dev
  cp -avp /dev/ram2   dev
  cp -avp /dev/ram3   dev
  cp -avp /dev/random   dev
  cp -avp /dev/stderr   dev
  cp -avp /dev/stdin   dev
  cp -avp /dev/stdout   dev
  cp -avp /dev/tty   dev
  cp -avp /dev/tty0   dev
  cp -avp /dev/tty1   dev
  cp -avp /dev/tty2   dev
  cp -avp /dev/tty3   dev
  cp -avp /dev/tty4   dev
  cp -avp /dev/tty5   dev
  cp -avp /dev/tty6   dev
  cp -avp /dev/tty7   dev
  cp -avp /dev/tty8   dev
  cp -avp /dev/tty9   dev
  cp -avp /dev/urandom   dev
  cp -avp /dev/vcs   dev
  cp -avp /dev/zero dev
